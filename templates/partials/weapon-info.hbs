<div class="weapon">
  <li class="item flexrow" data-item-id="{{_id}}">
    <tr>
      <td>
        <img src="{{img}}" title="{{name}}" width="24" height="24"/>
      </td>
      <td>
        <span>{{name}}</span>
      </td>
      <td>
        <span>{{getTextFromKey "wh3e.damageDice" data.damage}}</span>
      </td>
      <td>
        <span>{{getTextFromKey "wh3e.weightTypes" data.weight}}</span>
      </td>
      <td>
        <span>{{data.range}}</span>
      </td>
      <td>
        <span>{{getTextFromKey "wh3e.roFShotsPerRound" data.rateOfFire}}</span>
      </td>
      <td>
          <a class="item-control item-edit" title="Edit Item"><i class="fas fa-edit"></i></a>
          <a class="item-control item-delete" title="Delete Item"><i class="fas fa-trash"></i></a>        
      </td>
    </tr>
  </li>
</div>
